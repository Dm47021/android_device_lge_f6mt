SectionUseCase."Voice Call" {
Comment "Voice Call use case"
SectionVerb
	Name "Voice Call"

	EnableSequence
	'SLIM_0_RX_Voice Mixer CSVoice':1:1
	'Voice_Tx Mixer SLIM_0_TX_Voice':1:1
	EndSequence

	DisableSequence
	'SLIM_0_RX_Voice Mixer CSVoice':1:0
	'Voice_Tx Mixer SLIM_0_TX_Voice':1:0
	EndSequence
	
	# ALSA PCMs
	PlaybackPCM 2
	CapturePCM 2
	ACDBID 0:0

EndSection
SectionVerb
	Name "Voice CallSpeaker"

	EnableSequence
	EndSequence

	DisableSequence
		'SLIM_0_RX_Voice Mixer CSVoice':1:0
		'Voice_Tx Mixer SLIM_0_TX_Voice':1:0
	EndSequence

	# ALSA PCMs
	PlaybackPCM 2
	CapturePCM 2
	ACDBID 0:0

EndSection



